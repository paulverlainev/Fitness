<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        Icon="AvaRes.logo-abd.ico"
        x:Class="FitnessCenter.MainWindow"
        Title="Авторизация">
        <StackPanel Orientation="Horizontal">
                <StackPanel Orientation="Vertical">
                        <StackPanel>
                                <TextBlock>Логин:</TextBlock>
                                <TextBox Name="Log" Width="200"></TextBox>
                                <TextBlock>Пароль:</TextBlock>
                                <TextBox Name="Pass" Width="200"></TextBox>
                        </StackPanel>
        
                        <StackPanel>
                                <Button Name="Auto" Content="Войти" Click="Autorization"></Button>
                                <Button Name="Reg" Content="Зарегистрироваться" Click="Registration"></Button>
                         </StackPanel>
                </StackPanel>
        </StackPanel>
</Window>
